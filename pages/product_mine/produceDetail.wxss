page {
    background-color: #efeff5;
}

image {
    display: block;
    width: auto;
    height: auto;
}

.annouce {
    width: 100%;
    height: 80rpx;
    font-size: 14px;
    color: #f1312f;
    background-color: #fdfdec;
    line-height: 80rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    border: 1px solid #d7d7da;
}

.annouce image {
    width: 42rpx;
    height: 42rpx;
    margin-left: 30rpx;
    margin-right: 20rpx;
}

.phoneInput {
    width: 570rpx;
    height: 180rpx;
    float: left;
    background-color: #fff;
}

.border {
    width: 750rpx;
    height: 90rpx;
    background: #fff;
    border-bottom: 1px solid #d2d2d2;
    border-top: 1px solid #d2d2d2;
    margin: 20rpx auto;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.border input {
    width: 570rpx;
    padding-left: 30rpx;
}

.inputSave {
    width: 180rpx;
    height: 90rpx;
    color: #fff;
    line-height: 90rpx;
    text-align: center;
}

.page_avatar {
    width: 100%;
    height: 230rpx;
    position: relative;
    background: #f8f8f8;
}

.page_avatar .page_avatar_icon {
    width: 148rpx;
    height: 158rpx;
    padding: 36rpx;
    float: left;
}

.page_avatar .page_avatar_icon view {
    width: 150rpx;
    height: 150rpx;
    overflow: hidden;
    position: relative;
}

.page_avatar .page_avatar_icon view image {
    position: absolute;
    left: -58rpx;
    top: 0;
    width: 266rpx;
    height: 150rpx;
}

.page_avatar_inner {
    width: 520rpx;
    height: 230rpx;
    display: flex;
    justify-content: flex-start;
    align-items: flex-start;
    flex-direction: column;
}

.page_avatar_inner .inner_name {
    width: 354rpx;
    line-height: 36rpx;
    font-size: 16px;
    color: #000;
    margin-top: 42rpx;
    margin-bottom: 20rpx;
    float: left;
}

.page_avatar_inner .inner_desc {
    width: 480rpx;
    font-size: 14px;
    color: #f3312d;
    float: left;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.inner_desc_type {
    width: 170rpx;
    height: 56rpx;
    text-align: right;
    line-height: 56rpx;
    font-size: 16!important;
    color: #999;
    float: right;
}

.page_avatar_inner .inner_desc view {
    font-size: 16px;
}

.page_avatar_inner .inner_desc text {
    font-size: 18px;
    color: #f3312d;
    flex: 1;
}

.Num_count {
    width: 196rpx;
    height: 56rpx;
}

.Num_count .Num_count_btn {
    width: 62rpx;
    height: 56rpx;
    text-align: center;
    line-height: 56rpx;
    font-size: 24px;
    float: left;
    background: #f5f5f5;
    font-weight: bold;
    color: #959595;
}

.Num_count .Num_count_screen {
    width: 72rpx;
    height: 56rpx;
    text-align: center;
    line-height: 56rpx;
    float: right;
    font-size: 16px;
}

.btn_choosed {
    width: 16rpx;
    height: 16rpx;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    background-color: #f23030;
    border: 4rpx solid #f99a9a;
    margin-right: 60rpx;
}

.btn_nochoose {
    width: 18rpx;
    height: 18rpx;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    background-color: #fff;
    border: 2rpx solid #afafaf;
    margin-right: 60rpx;
}

.cell__bd text {
    font-size: 16px;
}

.cell__ft text {
    font-size: 16px;
}

.footer {
    width: 100%;
    height: 98rpx;
    border-top: 2rpx solid #d2d2d2;
    background-color: #fff;
    position: absolute;
    bottom: 0;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.price_pai {
    margin-right: 30rpx;
}

.price_pai_num {
    color: #f23030;
}

.price_pai_num view {
    font-size: 12px;
    float: left;
    line-height: 40rpx;
}

.price_pai_num text {
    float: left;
}

.descition {
    margin-left: 30rpx;
    margin-top: 20rpx;
    display: flex;
    justify-content: flex-start;
    align-items: flex-start;
    flex-direction: column;
}

.descition text {
    color: #555;
    font-size: 14px;
}

.cancel_btn {
    width: 120rpx;
    height: 98rpx;
    text-align: left;
    line-height: 98rpx;
    float: left;
    color: #555;
    margin-left: 30rpx;
    font-size: 14px;
    flex: 1;
}

.bought_btn {
    width: 140rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    float: left;
    color: #fff;
    font-size: 14px;
    background-color: #f23030;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    margin-right: 30rpx;
}

.footer_money {
    float: right;
    font-size: 16px;
    margin-right: 24rpx;
    margin-left: 30rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.gray_hover {
    background: #eaeaea;
}

.f8::after {
    background-color: #f8f8f8!important;
}