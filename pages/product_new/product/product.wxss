.swiper-tab {
    height: 90rpx;
    white-space: nowrap;
    display: flex;
    overflow: auto;
    background-color: #fff;
    margin-bottom: 20rpx;
}

.swiper-tab-list {
    font-size: 30rpx;
    display: inline-block;
    color: #000;
    padding: 24rpx 24rpx;
    margin-left: 16rpx;
}

.on {
    padding: 12rpx 24rpx;
    color: #f1312f;
    border-bottom: 8rpx solid #f1312f;
}

.swiper-box {
    display: block;
    height: 100%;
    width: 100%;
    overflow: hidden;
}

.shoppingList {
    width: 100%;
    height: 624rpx;
    background-color: #fff;
    margin-bottom: 20rpx;
}

.shoppingList image {
    width: 710rpx;
    height: 400rpx;
    margin: 20rpx;
}

.shoppingList .shoppingList_title {
    margin: -10rpx 20rpx 10rpx;
    font-size: 18px;
    width: 100%;
    float: left;
}

.shoppingList .shoppingList_PriceStart {
    width: 30%;
    margin: 0rpx 20rpx;
    float: left;
    font-size: 14px;
    color: #999;
}

.shoppingList .shoppingList_PriceEnd {
    width: 100%;
    margin: 0 20rpx;
    float: left;
    font-size: 14px;
    color: #ef312e;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.shoppingList .shoppingList_PriceEnd .PriceEndNum {
    font-size: 24px;
    /*font-weight: bold;*/
}

.shoppingList .shoppingList_PriceEnd .SupeValue {
    font-size: 10px;
    font-weight: bold;
    padding: 0 5px;
    color: #ef312e;
    border: 1px solid #ef312e;
    margin-left: 10rpx;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.shoppingList .shoppingList_PriceEnd .ShareFriend {
    color: #999;
}

.gray_hover {
    background: #eaeaea;
}


/*分享精度条*/

.share_panel {
    width: 100%;
    height: 44rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.share_times {
    width: 70%;
    height: 60rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.share_times_title {
    font-size: 14px;
    color: #999;
}

.share_times_border {
    width: 210rpx;
    height: 20rpx;
    background-color: #f4f4f4;
    border: 1px solid #dcdcdc;
    -webkit-border-radius: 18px;
    -moz-border-radius: 18px;
    border-radius: 18px;
    margin-left: 10rpx;
}

.share_times_inner {
    height: 20rpx;
    background-color: #f54d59;
    -webkit-border-radius: 18px;
    -moz-border-radius: 18px;
    border-radius: 18px;
}

.share_times_fen {
    width: 20%;
    height: 136rpx;
    display: block;
    color: #f54d59;
    font-size: 14px;
    margin-left: 10rpx;
}